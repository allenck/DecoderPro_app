   <variable label="Smoke puffs at speed 1" CV="353" tooltip="CV353 (0-255)" default="120" item="Sound Option 3">
	<decVal min="0" max="255"/>
	<label xml:lang="fr">Poussée de fumée en vitesse 1</label>
	<label xml:lang="de">Dampfstöße bei Fahrstufe 1</label>
   </variable>
  <variable label="Smoke puffs at higher speed step" CV="354" tooltip="CV354 (0-255)" default="20" item="Sound Option 4">
	<decVal min="0" max="255"/>
	<label xml:lang="fr">Poussée de fumée en vitesse supérieure</label>
	<label xml:lang="de">Dampfstöße bei höherer Fahrstufe</label>
   </variable>
   <variable label="Breaking noise at minimal speed step" CV="355" tooltip="CV355 (0-127)" default="20" item="Sound Option 11">
    <decVal min="0" max="127"/>
    <label xml:lang="fr">Bruit freinage vitesse minimale</label>
    <label xml:lang="de">Bremsenquietschen minimale Fahrstufe</label>
  </variable>
  <variable label="Breaking noise at start up speed step" CV="356" tooltip="CV356 (0-127)" default="13" item="Sound Option 12">
    <decVal min="0" max="127"/>
    <label xml:lang="fr">Bruit freinage vitesse initiale</label>
    <label xml:lang="de">Bremsenquietschen Anfangsfahrstufe</label>
  </variable>
  <variable label="Modulation at speed step 1" CV="357" tooltip="CV357" readOnly="yes" item="Sound Option 5">
    <decVal/>
    <label xml:lang="fr">Modulation cran de vitesse 1</label>
    <label xml:lang="de">Modulation bei Fahrstufe 1</label>
  </variable>
  <variable label="Modulation at speed step 127" CV="358" tooltip="CV358 (0-255)" default="11" item="Sound Option 6">
     <decVal min="0" max="255"/>
    <label xml:lang="fr">Modulation cran de vitesse 127</label>
    <label xml:lang="de">Modulation bei Fahrstufe 127</label>
  </variable>
  <variable label="Fade-out time in seconds" CV="359" tooltip="CV359 (0-255 in seconds)" default="8" item="Sound Option 7">
     <decVal min="0" max="255"/>
    <label xml:lang="fr">Délai pour mode silencieux</label>
    <label xml:lang="de">Timer für Ausblendeffekt</label>
  </variable>
</variables>