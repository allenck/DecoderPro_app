  <aspects>
    <aspect>
      <name>Stop</name>
      <indication>Stop</indication>
      <speed>Stop</speed>
      <speed2>Stop</speed2>
      <route>Either</route>
      <dccAspect>0</dccAspect>
    </aspect>
    <aspect>
      <name>Stop for Engineering</name>
      <indication>Stop</indication>
      <speed>Stop</speed>
      <speed2>Stop</speed2>
      <route>Either</route>
      <dccAspect>4</dccAspect>
    </aspect>
    <aspect>
      <name>Clear</name>
      <indication>Proceed</indication>
      <speed>Normal</speed>
      <speed2>Normal</speed2>
      <route>Normal</route>
      <dccAspect>2</dccAspect>
    </aspect>
    <aspect>
      <name>Approach</name>
      <indication>Proceed preparing to stop at next signal</indication>
      <speed>Normal</speed>
      <speed2>Limited</speed2>
      <route>Normal</route>
      <dccAspect>1</dccAspect>
    </aspect>
    <aspect>
      <name>Limited</name>
      <indication>Proceed at Limited speed</indication>
      <speed>Limited</speed>
      <speed2>Limited</speed2>
      <route>Normal</route>
      <dccAspect>6</dccAspect>
    </aspect>
    <aspect>
      <name>Restricting</name>
      <indication>Proceed at restricted speed because line may be blocked</indication>
      <speed>Normal</speed>
      <speed2>Slow</speed2>
      <route>Normal</route>
      <dccAspect>5</dccAspect>
    </aspect>
    <aspect>
      <name>Not Lit</name>
      <rule>Dark</rule>
      <indication>(used to indicate dark for e.g. approach lighting)</indication>
      <speed>Stop</speed>
      <speed2>Stop</speed2>
      <route>Normal</route>
      <dccAspect>8</dccAspect>
    </aspect>
  </aspects>
  <imagetypes>
    <imagetype type="large" />
    <imagetype type="aspects" />
  </imagetypes>
  <appearancefiles>
    <appearancefile href="appearance-High.xml" />
    <appearancefile href="appearance-Dwarf.xml" />
  </appearancefiles>
</aspecttable>