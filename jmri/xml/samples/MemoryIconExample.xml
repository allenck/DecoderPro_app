    <memory systemName="IM20" userName="Image Icons" />
  </memories>
  <paneleditor class="jmri.jmrit.display.configurexml.PanelEditorXml" name="Test memory" x="846" y="22" height="382" width="280" editable="yes" positionable="yes" controlling="yes" hide="no" panelmenu="yes">
    <memoryicon memory="IM10" x="10" y="10" text="A" level="5" size="12" style="0" selectable="no" class="jmri.jmrit.display.configurexml.MemoryIconXml" defaulticon="resources/icons/misc/X-red.gif" />
