<!-- This is an example panel containing MemoryIcons to display   -->
<!-- text and images as the value changes.  To see it work,       -->
<!-- go to the Memory Table Tool and change the values.           -->
<layout-config>
  <memories class="jmri.configurexml.DefaultMemoryManagerXml">
    <memory systemName="IM10" userName="Icon with Black Text" />
    <memory systemName="IM11" userName="Icon with Large Text" />
